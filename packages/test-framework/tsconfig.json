{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@api/*": ["src/api*"],
      "@config/*": ["src/config/*"],
      "@fixtures/*": ["src/fixtures/*"],
      "@pages/*": ["src/pages/*"],
      "@support/*": ["src/support/*"],
      "@tests/*": ["src/tests/*"],
      "@utils/*": ["src/utils/*"],
      "@validators/*": ["src/validators/*"]
    }
  },
  "include": ["src/**/*"]
}
